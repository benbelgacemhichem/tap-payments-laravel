{
  "name": "groupedesign/laravel-tap-payments",
  "description": "Tap Payment REST API package for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Benbelgacem Hichem",
      "email": "benbelgacemhichem@gmail.com",
      "homepage": "http://www.linkia.qa/"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*|7.*|8.*",
    "guzzlehttp/guzzle": ">=6.0"
  },
  "autoload": {
    "psr-4": {
      "Groupedesign\\TapPayment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Groupedesign\\TapPayment\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Groupedesign\\TapPayment\\TapPaymentServiceProvider"
      ]
    }
  }
}